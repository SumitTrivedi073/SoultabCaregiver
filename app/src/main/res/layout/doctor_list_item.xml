<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/doctor_list_relative"
    android:layout_width="match_parent"
    android:layout_height="@dimen/_40sdp"
    android:layout_marginTop="@dimen/_10sdp"
    android:layout_marginStart="@dimen/_10sdp"
    android:layout_marginLeft="@dimen/_10sdp"
    android:layout_marginEnd="@dimen/_10sdp"
    android:layout_marginRight="@dimen/_10sdp"
    android:background="@drawable/popup_relative">

    <RelativeLayout
        android:id="@+id/o_realtive"
        android:layout_width="@dimen/_15sdp"
        android:layout_height="match_parent"
        android:background="@drawable/left_bottom_corner_orange"/>

    <TextView
            android:id="@+id/doctor_name"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textSize="@dimen/_12sdp"
            android:textColor="@color/blackish"
            android:text="My First Task"
            android:fontFamily="@font/muli_bold"
            android:gravity="center|start"
            android:layout_gravity="center"
            android:maxLines="2"
            android:ellipsize="end"
            android:layout_centerVertical="true"
            android:layout_marginStart="@dimen/_10sdp"
            android:layout_marginLeft="@dimen/_10sdp"
            android:layout_toEndOf="@+id/o_realtive"
            android:layout_toRightOf="@+id/o_realtive"
            android:layout_toStartOf="@+id/btn_schedule"
            android:layout_toLeftOf="@+id/btn_schedule" />

    <!--<RelativeLayout
        android:layout_width="@dimen/_30sdp"
        android:layout_height="match_parent"
        android:layout_toStartOf="@+id/btn_schedule"
        android:layout_toLeftOf="@+id/btn_schedule" >




    </RelativeLayout>-->

    <TextView
        android:id="@+id/btn_schedule"
        android:layout_width="@dimen/_80sdp"
        android:layout_height="@dimen/_30sdp"
        android:layout_centerInParent="true"
        android:background="@drawable/muzli_btn_bg"
        android:fontFamily="@font/muli_bold"
        android:gravity="center"
        android:text="@string/schedule"
        android:textColor="@color/white"
        android:textSize="@dimen/_10sdp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginEnd="@dimen/_5sdp"
        android:layout_marginRight="@dimen/_5sdp" />



</RelativeLayout>